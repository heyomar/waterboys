{
  "name": "homosaur/hoboPress",
  "description": "Vagrant workflow for WP",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Paul Graham",
      "email": "paul@bytefair.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=5.5",
    "composer/installers": "^1.0",
    "johnpbloch/wordpress": "^4.4.2",
    "vlucas/phpdotenv": "~2.2",
    "wpackagist-plugin/disable-comments": "^1.4",
    "wpackagist-plugin/regenerate-thumbnails": "^2.2",
    "wpackagist-plugin/wordpress-seo": "^3.1",
    "wpackagist-plugin/wp-smushit": "^2.1",
    "wpackagist-plugin/enable-media-replace": "^3.0",
    "wpackagist-plugin/simple-page-ordering": "^2.2",
    "wpackagist-plugin/disable-emojis": "^1.5",
    "wpackagist-plugin/ninja-forms": "^2.9",
    "wpackagist-plugin/custom-post-type-ui": "^1.3",
    "wpackagist-plugin/white-label-cms": "^1.5",
    "catfan/Medoo": "^1.1",
    "stripe/stripe-php": "^3.17",
    "mandrill/mandrill": "^1.0"
  },
  "require-dev": {
    "wpackagist-plugin/query-monitor": "^2.8",
    "wpackagist-plugin/debug-objects": "^2.2",
    "wpackagist-plugin/user-switching": "^1.0",
    "wpackagist-plugin/debug-bar": "^0.8.2",
    "wpackagist-plugin/rewrite-rules-inspector": "^1.2",
    "wpackagist-plugin/log-deprecated-notices": "^0.3.0",
    "wpackagist-plugin/theme-check": "^20151211.1"
  },
  "extra": {
    "installer-paths": {
      "app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "app/plugins/{$name}/": ["type:wordpress-plugin"],
      "app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "wp"
  }
}
